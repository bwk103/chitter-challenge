<li class="user">
  <div class="card">
    <div class="card-block">
      <div class="card-body">
        <h5 class="card-title"><%= user.name %>
          <img id="profile_image" src="<%= user.profile_image_url %>">
        </h5>
        <h6 class="text-muted"> @<%= user.username %></span></h6>
      </div>
    </div>
  </div>
</li>
